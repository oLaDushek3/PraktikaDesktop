<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" Height="150" Width="300"
             x:Class="PraktikaDesktop.Views.Dialog.BaseDialogView"
			 xmlns:vm="using:PraktikaDesktop.ViewModels.Dialog"
			 d:DataContext="{x:Static vm:BaseDialogViewModel.BaseDialogViewModel}">

	<Grid Background="{StaticResource primaryBackColor2}">
		<ContentControl Content="{Binding CurrentDialogView}"/>
	</Grid>
	
</UserControl>
